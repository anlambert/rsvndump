#: src/delta.c:315
#, c-format
msgid "ERROR: Failed to open %s.\n"
msgstr "FEHLER: Kann %s nicht öffnen.\n"

#: src/dump.c:136
#, c-format
msgid "ERROR: URL '%s' not found in HEAD revision\n"
msgstr "FEHLER: URL '%s' wurde in der HEAD-Revision nicht gefunden\n"

#: src/dump.c:242 src/dump.c:249 src/dump.c:254
#, c-format
msgid "ERROR: URL '%s' not found in revision %ld\n"
msgstr "FEHLER: URL '%s' wurde in Revision %ld nicht gefunden\n"

#: src/dump.c:365
#, c-format
msgid "* Dumped revision %ld.\n"
msgstr " * Revision %ld gedumpt.\n"

#: src/log.c:151
#, c-format
msgid "Determining start end end revision... "
msgstr "Bestimme Anfangs- und Endrevision... "

#: src/log.c:164 src/log.c:237
#, c-format
msgid "done\n"
msgstr "fertig\n"

#: src/log.c:222
#, c-format
msgid "Fetching logs... "
msgstr "Frage Logs ab... "

#: src/main.c:57
#, c-format
msgid "USAGE:"
msgstr "AUFRUF:"

#: src/main.c:59
#, c-format
msgid ""
"[options] <url>\n"
"\n"
msgstr ""
"[Optionen] <Url>\n"
"\n"

#: src/main.c:60
#, c-format
msgid "Valid options:\n"
msgstr "Optionen:\n"

#: src/main.c:61
#, c-format
msgid "    -h [--help]               print a nice help screen\n"
msgstr "    -h [--help]               Diese Hilfe anzeigen\n"

#: src/main.c:62
#, c-format
msgid "    --version                 print the program name and version\n"
msgstr ""
"    --version                 Name und Version des Programms ausgeben\n"

#: src/main.c:63
#, c-format
msgid "    -q [--quiet]              be quiet\n"
msgstr "    -q [--quiet]              Nur Fehler ausgeben\n"

#: src/main.c:64
#, c-format
msgid "    -v [--verbose]            print extra progress\n"
msgstr "    -v [--verbose]            Zusätzliche Informationen ausgeben\n"

#: src/main.c:65
#, c-format
msgid "    -u [--username] arg       username\n"
msgstr "    -u [--username] arg       Benutzername\n"

#: src/main.c:66
#, c-format
msgid "    -p [--password] arg       password\n"
msgstr "    -p [--password] arg       Passwort\n"

#: src/main.c:67
#, c-format
msgid "    -r [--revision] arg       specify revision number (or X:Y range)\n"
msgstr "    -r [--revision] arg       Revisionsnummer (oder X:Y Bereich)\n"

#: src/main.c:68
#, c-format
msgid "    --deltas                  use deltas in dump output\n"
msgstr "    --deltas                  deltas im dump verwenden\n"

#: src/main.c:69
#, c-format
msgid "    --incremental             dump incrementally\n"
msgstr "    --incremental             einen inkrementellen dump erstellen\n"

#: src/main.c:70
#, c-format
msgid "    --no-check-certificate    don't validate SSL certificates\n"
msgstr "    --no-check-certificate    SSL-Zertifikate nicht validieren\n"

#: src/main.c:71
#, c-format
msgid ""
"    --prefix arg              prepend arg to the path that is being dumped\n"
msgstr "    --prefix arg              Zuzsätzlicher Präfix für Pfade\n"

#: src/main.c:72
#, c-format
msgid ""
"    --keep-revnums            keep the dumped revision numbers in sync with\n"
"                              the repository by using empty revisions for\n"
"                              padding\n"
msgstr ""
"    --keep-revnums            Revisions-Nummern nicht verändern, indem wenn\n"
"                              nötig leere Revisionen eingefügt werden\n"

#: src/main.c:75
#, c-format
msgid "    --dump-uuid               include the repository uuid in the dump\n"
msgstr ""
"    --dump-uuid               die UUID des repositories ebenfalls dumpen\n"

#: src/main.c:183
#, c-format
msgid "ERROR: invalid revision range '%s'\n"
msgstr "FEHLER: Ungültiger Revisions-Bereich '%s'\n"

#: src/main.c:209
#, c-format
msgid "ERROR: multiple URLs detected.\n"
msgstr "FEHLER: Meherere URLs entdeckt.\n"

#: src/main.c:216
#, c-format
msgid "ERROR: Unkown argument or malformed url '%s'.\n"
msgstr "FEHLER: Ungültiges Argument oder ungültige URL '%s'.\n"

#: src/main.c:217
#, c-format
msgid "Type %s --help for usage information.\n"
msgstr "Rufen sie %s --help auf, um Informationen zur Benutzung zu erhalten.\n"

#: src/main.c:245
#, c-format
msgid "ERROR: Unable to create download directory.\n"
msgstr "FEHLER: Kann Verzeichnis für die working copy nicht erstellen.\n"

#: src/session.c:108
msgid "Username: "
msgstr "Benutzername: "

#: src/session.c:113
#, c-format
msgid "Password for '%s': "
msgstr "Passwort für '%s': "

#: src/session.c:117
msgid "Password: "
msgstr "Passwort: "

#. TRANSLATORS: This is the key used for accepting a certificate temporarily.
#. * Every other key is used to reject the certificate.
#: src/session.c:133
msgid "t"
msgstr "t"

#: src/session.c:137 src/session.c:146 src/session.c:154 src/session.c:162
#: src/session.c:170
#, c-format
msgid "WARNING: Validating server certificate for '%s' failed:\n"
msgstr "WARNUNG: Zertifikat für '%s' konnte nicht validiert werden:\n"

#: src/session.c:139 src/session.c:148 src/session.c:156 src/session.c:164
#: src/session.c:172
#, c-format
msgid "Error validating server certificate for '%s':\n"
msgstr "Fehler: Zertifikat für '%s' konnte nicht validiert werden:\n"

#: src/session.c:141
#, c-format
msgid ""
" - The certificate is not issued by a trusted authority. Use the\n"
"   fingerprint to validate the certificate manually!\n"
msgstr ""
" - Das Zertifikat is nicht von einer zuverlässigen Zertifizierungsstelle\n"
"   ausgestellt wordem. Benutzen Sie den Fingerprint, um das Zertifikat\n"
"   manuell zu verifizieren!\n"

#: src/session.c:150
#, c-format
msgid " - The certificate hostname does not match.\n"
msgstr " - Der Hostname des Zertifikats passt nicht.\n"

#: src/session.c:158
#, c-format
msgid " - The certificate is not yet valid.\n"
msgstr " - Das Zertifikat is noch nicht gültig.\n"

#: src/session.c:166
#, c-format
msgid " - The certificate has expired.\n"
msgstr " - Das Zertifikat ist abgelaufen.\n"

#: src/session.c:174
#, c-format
msgid " - The certificate has an unknown error.\n"
msgstr " - Das Zertifikat besitzt einen unbekannten Fehler.\n"

#: src/session.c:177
#, c-format
msgid ""
"Certificate information:\n"
" - Hostname: %s\n"
" - Valid: from %s until %s\n"
" - Issuer: %s\n"
" - Fingerprint: %s\n"
msgstr ""
"Informatioen zum Zertifikat:\n"
" - Hostname: %s\n"
" - Gültig: von %s bis %s\n"
" - Aussteller: %s\n"
" - Fingerprint: %s\n"

#: src/session.c:195
#, c-format
msgid "(R)eject or accept (t)emporarily? "
msgstr "A(b)lehnen oder (t)emporär akzeptieren? "

