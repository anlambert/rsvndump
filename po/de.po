#: src/main.c:56
#, c-format
msgid "USAGE:"
msgstr "AUFRUF:"

#: src/main.c:58
#, c-format
msgid ""
"[options] <url>\n"
"\n"
msgstr ""
"[Optionen] <Url>\n"
"\n"

#: src/main.c:59
#, c-format
msgid "Valid options:\n"
msgstr "Optionen:\n"

#: src/main.c:60
#, c-format
msgid "    -h [--help]               print a nice help screen\n"
msgstr "    -h [--help]               Diese Hilfe anzeigen\n"

#: src/main.c:61
#, c-format
msgid "    --version                 print the program name and version\n"
msgstr ""
"    --version                 Name und Version des Programms ausgeben\n"

#: src/main.c:62
#, c-format
msgid "    -q [--quiet]              be quiet\n"
msgstr "    -q [--quiet]              Nur Fehler ausgeben\n"

#: src/main.c:63
#, c-format
msgid "    -v [--verbose]            print extra progress\n"
msgstr "    -v [--verbose]            Zusätzliche Informationen ausgeben\n"

#: src/main.c:64
#, c-format
msgid "    -u [--username] arg       username\n"
msgstr "    -u [--username] arg       Benutzername\n"

#: src/main.c:65
#, c-format
msgid "    -p [--password] arg       password\n"
msgstr "    -p [--password] arg       Passwort\n"

#: src/main.c:66
#, c-format
msgid "    -o [--outfile] arg        write data to file arg\n"
msgstr ""
"    -o [--outfile] arg        Datei 'arg' für Ausgabe der Daten benutzen\n"

#: src/main.c:67
#, c-format
msgid "                              if not specified, print to stdout\n"
msgstr ""
"                              (ansonsten wird die Standardausgabe benutzt\n"

#: src/main.c:68
#, c-format
msgid "    -d [--download-dir] arg   directory for working copy\n"
msgstr "    -d [--download-dir] arg   Verzeichnes für die working copy\n"

#: src/main.c:69
#, c-format
msgid ""
"                              if not specified, create a temporary "
"directory\n"
msgstr ""

#: src/main.c:70
#, c-format
msgid "    --online                  don't use a working copy for dumping\n"
msgstr ""

#: src/main.c:71
#, c-format
msgid "    --no-check-certificate    don't validate SSL certificates\n"
msgstr "    --no-check-certificate    SSL-Zertifikate nicht validieren\n"

#: src/main.c:72
#, c-format
msgid "    --stop arg                stop after dumping revision arg\n"
msgstr ""
"    --stop arg                Bis einschließlich Revision 'arg' dumpen\n"

#: src/main.c:73
#, c-format
msgid ""
"    --prefix arg              prepend arg to the path that is being dumped\n"
msgstr "    --prefix arg              Zuzsätzlicher Präfix für Pfade\n"

#: src/main.c:74
#, c-format
msgid ""
"    --keep-revnums            keep the dumped revision numbers in sync with\n"
msgstr ""

#: src/main.c:75
#, c-format
msgid ""
"                              the repository by using empty revisions for\n"
msgstr ""

#: src/main.c:76
#, c-format
msgid "                              padding\n"
msgstr ""

#: src/main.c:77
#, c-format
msgid "    --dump-uuid               include the repository uuid in the dump\n"
msgstr ""

#: src/main.c:79
#, c-format
msgid "    --deltas                  use deltas in dump output\n"
msgstr ""

#: src/main.c:175
#, c-format
msgid "Error: Unable to open output file.\n"
msgstr "Fehler: Ausgabedatei kann nicht geöffnet werden.\n"

#: src/main.c:183
#, c-format
msgid "Error: '%s' is either not a directory or does not exist.\n"
msgstr "Fehler: '%s' ist entweder kein Verzeichnis oder existiert nicht.\n"

#: src/main.c:193
#, c-format
msgid "Error: multiple URLs detected.\n"
msgstr "Fehler: meherere URLs entdeckt.\n"

#: src/main.c:199
#, c-format
msgid "Argument error: Unkown argument or malformed url '%s'\n"
msgstr "Fehler: Ungültiges argument oder ungültige URL '%s'\n"

#: src/main.c:200
#, c-format
msgid "Type %s --help for usage information\n"
msgstr "Rufen sie %s --help für Informationen zur Benutzung auf\n"

#: src/main.c:225
#, c-format
msgid "Error: Unable to create download directory.\n"
msgstr "Fehler: Kann Verzeichnis für die working copy nicht erstellen.\n"
